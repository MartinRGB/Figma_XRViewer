{
	"name": "XRViewer",
	"version": "0.2.0",
	"description": "This plugin template uses Typescript. If you are familiar with Javascript, Typescript will look very familiar. In fact, valid Javascript code is already valid Typescript code.",
	"license": "ISC",
	"scripts": {
		"dev": "npm-run-all -l -p dev:xrviewer build:watch_p_xrviewer build:watch_p_webxr build:watch_p_unity",
		"dev:xrviewer": "webpack serve --config ./webpack/webpack.xrviewer.js --mode=development --env PLUGIN=xrviewer",
		"build:all": "npm-run-all -l -p build:xrviewer build:plugin_xrviewer build:plugin_webxr build:plugin_unity",
		"build:plugin": "npm-run-all -l -p build:plugin_xrviewer build:plugin_webxr build:plugin_unity",
		"build:xrviewer": "webpack --config ./webpack/webpack.xrviewer.js --mode=production --env PLUGIN=xrviewer",
		"build:plugin_xrviewer": "webpack --config ./webpack/webpack.plugin.js --mode=production --env PLUGIN=xrviewer",
		"build:watch_p_xrviewer": "webpack --config ./webpack/webpack.plugin.js --mode=development --env PLUGIN=xrviewer --watch",
		"build:plugin_webxr": "webpack --config ./webpack/webpack.plugin.js --mode=production --env PLUGIN=webxr",
		"build:watch_p_webxr": "webpack --config ./webpack/webpack.plugin.js --mode=development --env PLUGIN=webxr --watch",
		"build:plugin_unity": "webpack --config ./webpack/webpack.plugin.js --mode=production --env PLUGIN=unity",
		"build:watch_p_unity": "webpack --config ./webpack/webpack.plugin.js --mode=development --env PLUGIN=unity --watch",
		"prettier:format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,json}' ",
		"release": "rm XRViewer/code.js && rm XRViewer/ui.html && rm XRViewer_WebXR/code.js && rm XRViewer_WebXR/ui.html && rm XRViewer_Unity/code.js && rm XRViewer_Unity/ui.html && rm -rf release && mkdir release && echo all files cleaned && npm run build:plugin_xrviewer && npm run build:plugin_webxr && npm run build:plugin_unity && rm XRViewer/ui.js.LICENSE.txt && rm XRViewer_WebXR/ui.js.LICENSE.txt && rm XRViewer_Unity/ui.js.LICENSE.txt && echo build finished && bestzip release/XRViewer.zip XRViewer/* && bestzip release/XRViewer_WebXR.zip XRViewer_WebXR/* && bestzip release/XRViewer_Unity.zip XRViewer_Unity/* && echo figma plugin zipped && bestzip release/mac_script.zip cli/mac_script/* && echo mac script zipped && bestzip release/win_script.zip cli/win_script/* && echo win script zipped && dotenv release-it --ci --github.autoGenerate"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-flow": "^7.18.6",
		"@babel/preset-react": "^7.18.6",
		"@figma/plugin-typings": "^1.55.1",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.9",
		"@react-three/drei": "^9.43.2",
		"@react-three/fiber": "^8.9.1",
		"@react-three/xr": "^5.0.5",
		"@theatre/core": "^0.5.0",
		"@theatre/r3f": "^0.5.0",
		"@theatre/studio": "^0.5.0",
		"@types/file-saver": "^2.0.5",
		"@types/node": "^18.11.9",
		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.0.9",
		"@types/three": "^0.146.0",
		"babel-loader": "^9.1.0",
		"bestzip": "^2.2.1",
		"css-loader": "^6.7.2",
		"dotenv-cli": "^6.0.0",
		"file-saver": "^2.0.5",
		"html-inline-script-webpack-plugin": "^3.1.0",
		"html-webpack-plugin": "^5.5.0",
		"husky": "^8.0.2",
		"jszip": "^3.10.1",
		"lint-staged": "^13.0.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.0",
		"release-it": "^15.5.0",
		"style-loader": "^3.3.1",
		"styled-components": "^5.3.6",
		"three": "^0.146.0",
		"ts-loader": "^9.4.1",
		"tsconfig-paths-webpack-plugin": "^4.0.0",
		"typescript": "^4.9.3",
		"url-loader": "^4.1.1",
		"webpack": "^5.75.0",
		"webpack-bundle-analyzer": "^4.7.0",
		"webpack-cli": "^5.0.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,css,json}": [
			"prettier --write",
			"git add"
		]
	}
}
